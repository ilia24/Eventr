<div class="log_sign_wrapper">

  <div class="log_sign_container">

    <h2>LOG IN</h2>

    <%= form_tag sessions_path do %>

    <div class="form_field">
      <%= label_tag :email %>
      <%= email_field_tag :email, params[:email] %>
    </div>

    <div class="form_field">
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>

    <p>
      <%= submit_tag "EntR" %>
    </p>

    <%end %>

  </div>

</div>
